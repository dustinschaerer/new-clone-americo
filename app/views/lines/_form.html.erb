    <% if (@qp_id == '6') || (@qp_id == '7') || (@qp_id == '8') || (@qp_id == '9') %>
      <%= f.hidden_field :quantity, placeholder: "Enter Quantity", value: "1" %>
    <% elsif (@qp_id == '4') || (@qp_id == '10') || (@qp_id == '11') || (@qp_id == '12') || (@qp_id == '17') %>
      <%= f.number_field :quantity, placeholder: "Enter Desired Number of Rolls" %>
    <% else %>
      <%= f.number_field :quantity, placeholder: "Enter Quantity" %>
    <% end %>
    
    <%= f.hidden_field :price, value: "0" %>

  </div>

  <div class="form-actions">
    <div class="form-group">
      <div class="col-sm-offset-3 col-sm-9">
        <%= f.submit 'Add Product to Quote Now', :class => "btn btn-success btn-block"  %>
      </div>
    </div>
  </div>
  


  <div class="form-group">
    <div class="col-sm-offset-3 col-sm-4">
      <%= link_to '/request_quote',  class: "btn btn-danger btn-block" do %>
      <span class="glyphicon glyphicon-arrow-left"></span>  Go Back 
