<%= bootstrap_form_for(@line, :url => { :action => :create, :quote_product_id => @qp_id }, style: :horizontal, left: "col-sm-3 col-md-3", right: "col-sm-9 col-md-9") do |f| %>

  <%= f.alert_message "Please fix the following errors:" %>

  
  <div class="form-inputs">
    <%= f.collection_select :quote_product_id, QuoteProduct.all.order("name"), :id, :name, { selected: @qp_id, label: false  } %>

    <% if (@qp_id == '1') || (@qp_id == '2') || (@qp_id == '16') %>  
    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="1|2|16">
      <%= f.collection_select :shape_id, Shape.all.order("id"), :id, :name, { prompt: "Please Choose" }   %>
    </div> 

    <% elsif (@qp_id == '15') %>
    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="15">
      <%= f.collection_select :shape_id, Shape.where("id = 2 or id = 3").order("id"), :id, :name, { prompt: "Please Choose" }   %>
    </div> 
    <% end %>

    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="1|2">
      <%= f.collection_select :cover_id, Cover.all.order("id"), :id, :name, { prompt: "Please Choose" } %>
    </div> 
    
    <% if (@qp_id == '4') %>  
    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="4">
      <%= f.select(:width, [[36, 36], [48, 48], [72, 72]], { prompt: "Please Choose"} )  %>
    </div>
    
    <% elsif (@qp_id == '15') %>  
    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="15">
      <%= f.select(:width, [['12"', 12], ['16"', 16], ['24"', 24]], { prompt: "Please Choose"} )  %>
    </div>

    <% else %>
    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="1|2|5|6|16">
      <%= f.number_field :width, placeholder: "Width (In Inches)" %>
    </div>
    <% end %>

    <% if (@qp_id == '5') %> 
    <div class="js-dependent-fields" data-select-id="line_quote_product_id" data-option-value="5">
      <%= f.number_field :length, placeholder: "Length (In Inches)" %>
    </div>
