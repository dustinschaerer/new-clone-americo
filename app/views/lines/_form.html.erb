    </div>  
    <div class="col-sm-offset-1 col-sm-4">
      <% unless @quoteholder.lines.empty? %>

        <%= link_to @quoteholder, class: "btn btn-info  btn-block", method: :get do %>
        Review my Quote <span class="glyphicon glyphicon-arrow-right"></span>
        <% end %>

      <% end %>


    </div>
  </div>
    


 

<% end %>

