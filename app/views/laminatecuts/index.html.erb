<h1>Listing laminatecuts</h1>

<table>
  <thead>
    <tr>
      <th>Quantity</th>
      <th>Yards long</th>
      <th>Series</th>
      <th>Color</th>
      <th>Price</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @laminatecuts.each do |laminatecut| %>
      <tr>
        <td><%= laminatecut.quantity %></td>
        <td><%= laminatecut.yards_long %></td>
        <td><%= laminatecut.series.name %></td>
        <td><%= laminatecut.color.name %></td>
        <td><%= laminatecut.price %></td>
        <td><%= link_to 'Show', laminatecut %></td>
        <td><%= link_to 'Edit', edit_laminatecut_path(laminatecut) %></td>
        <td><%= link_to 'Destroy', laminatecut, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Laminatecut', new_laminatecut_path %>
