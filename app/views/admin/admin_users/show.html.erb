<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <ol class="breadcrumb">
            <li>
                <a href="/admin"><i class="fa fa-dashboard"></i>  Dashboard</a>
            </li>
            <li>
                 <a href="/admin/admin_users"><i class="fa fa-bolt"></i> Administrators</a>
            </li>
            <li class="active">
               #<%= @admin_user.id %>
            </li>
        </ol>
    </div>
</div>
<!-- /.row -->

<p id="notice"><%= notice %></p>

<div class="bs-callout bs-callout-yellow">

	<div class="row">
	  <div class="col-lg-12">
	    <h4>ADMIN User ID# <%= @admin_user.id %></h4>
	    <hr>
	    <h4>Email: <%= @admin_user.email %></h4><br>
	    <table class="table table-striped table-hover">
	      <tr>
	      	<td>Created At:</td>
	      	<td><%= @admin_user.created_at.strftime("%B %d, %Y") %></td>
	      </tr>
	      <tr>
	      	<td>Updated At:</td>
	      	<td><%= @admin_user.updated_at.strftime("%B %d, %Y") %></td>
	      </tr>
	      <tr>
	      	<td>Sign in Count:</td>
	      	<td><%= @admin_user.sign_in_count %></td>
	      </tr>
	      <tr>
	      	<td>Last Sign in IP:</td>
	      	<td><%= @admin_user.last_sign_in_ip %></td>
	      </tr>
	      <tr>
	      	<td>Current Sign in IP:</td>
	      	<td><%= @admin_user.current_sign_in_ip %></td>
	      </tr>
	      <tr>
	      	<td>Encrypted Password:</td>
	      	<td><%= @admin_user.encrypted_password %></td>
	      </tr>

	    </table>


	  </div>
	</div>


</div>

<br><br><br><br><br><br><br><br><br><br>

